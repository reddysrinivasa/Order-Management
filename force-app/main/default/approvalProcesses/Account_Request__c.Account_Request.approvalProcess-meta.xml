<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>BillingCity__c</field>
        <field>BillingCountry__c</field>
        <field>BillingPostalCode__c</field>
        <field>BillingState__c</field>
        <field>BillingStreet__c</field>
        <field>Email__c</field>
        <field>FirstName__c</field>
        <field>PrimaryEmail__c</field>
        <field>LastName__c</field>
        <field>PrimaryPhone__c</field>
        <field>CreatedBy</field>
        <field>DOB__c</field>
        <field>Description__c</field>
        <field>LastModifiedBy</field>
        <field>Phone__c</field>
        <field>PrimaryTitle__c</field>
        <field>ShippingCity__c</field>
        <field>ShippingCountry__c</field>
        <field>ShippingPostalCode__c</field>
        <field>ShippingState__c</field>
        <field>ShippingStreet__c</field>
        <field>Status__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>team4@w.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Account_Request__c.Status__c</field>
                <operation>notEqual</operation>
                <value>Approved</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>Account Request</label>
        <name>Account_Request</name>
    </approvalStep>
    <emailTemplate>unfiled$public/Account_Request</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Account_Request_Approved</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Status_Field_Update</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Account_Request</name>
            <type>Alert</type>
        </action>
    </initialSubmissionActions>
    <label>Account Request</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
